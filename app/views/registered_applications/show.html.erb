<p id="notice"><%= notice %></p>

<%= link_to 'Home', root_path %> |
<%= link_to 'Edit this Application', edit_registered_application_path(@registered_application) %> |
<%= link_to 'My Registered Applications', registered_applications_path %>

<h1><%= @registered_application.name %></h1>

<div class="row">
  <div class="col-md-8">
    <h3><%= @registered_application.url %></h3>
    <% @events.each do |event_name, events| %>
      <p> <%= event_name %> : <%= events.size %>
        <%= events.size > 1 ? "events" : "event" %> </p>
    <% end %>
    <%= pie_chart @registered_application.events.group(:name).count %>
    <%= line_chart @registered_application.events.group_by_day(:created_at).count %>
  </div>
</div>
