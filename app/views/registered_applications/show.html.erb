<p id="notice"><%= notice %></p>

<%= link_to 'Home', root_path %> |
<%= link_to 'Edit this Application', edit_registered_application_path(@registered_application) %> |
<%= link_to 'My Registered Applications', registered_applications_path %>

<h1><%= @registered_application.name %></h1>

<div class="row">
  <div class="col-md-8">
    <h3><%= @registered_application.url %></h3>
    <%= @events.keys.each do |key| %>
      <p> <%= key %> : <%= key.length %> events </p>
    <% end %>
  </div>
</div>
