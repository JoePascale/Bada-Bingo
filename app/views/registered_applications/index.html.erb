<p id="notice"><%= notice %></p>

<h1>My Registered Applications</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registered_applications.each do |registered_application| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to registered_application.name, registered_application %>
          </h4>
          <p><%= link_to 'Edit', edit_registered_application_path(registered_application) %></p>
          <p><%= link_to 'Delete', registered_application, method: :delete, data: { confirm: 'Are you sure?' } %></p>
          <small>
            <%= registered_application.url %>
          </small>
        </div>
        <hr>
      </div>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registered application', new_registered_application_path %>
